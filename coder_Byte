function responsive(hover){
  
  
	switch(hover){
	case '1':
	
	var y = document.getElementsByTagName('div');
	for(var i=3;i<8;i++){
	y[i].style.background = "grey";
	}
	
	var y = document.getElementById('1');
	y.style.background="red";
    response = "Extremely Bad";
	break;
	
	
	case '2':
	var y = document.getElementsByTagName('div');
	for(var i=4;i<8;i++){
	y[i].style.background = "grey";
	}
	
	var y = document.getElementsByTagName('div');
	for(var i=3;i<5;i++){
	y[i].style.background = "#FF5733";
	}
	response = "Bad";
	break;
	
	
	case '3':
	
	var y = document.getElementsByTagName('div');
	for(var i=5;i<8;i++){
	y[i].style.background = "grey";
	}
	
	var y = document.getElementsByTagName('div');
	for(var i=3;i<6;i++){
	y[i].style.background = "orange";
	}
	response = "Natural";
	break;
	
	case '4':
	
	var y = document.getElementsByTagName('div');
	for(var i=6;i<8;i++){
	y[i].style.background = "grey";
	}
	
	var y = document.getElementsByTagName('div');
	for(var i=3;i<7;i++){
	y[i].style.background = "#97DC70";
	}
	response = "Good";
	break;
	
	case '5':
	
	var y = document.getElementsByTagName('div');
	for(var i=7;i<8;i++){
	y[i].style.background = "grey";
	}
	
	var y = document.getElementsByTagName('div');
	for(var i=3;i<8;i++){
	y[i].style.background = "green";
	}
	response = "Extremely Good";
	break;
	
	}
}

